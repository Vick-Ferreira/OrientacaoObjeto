package entidade;

public class Conversor {

	// declara~]ao de atributos e m√©todos com STATIC na subclasse permite , utilizar
	// sem importar a classe na principal
	public final double IMPOSTO = 6.0;
	public double quantidadeCompra;

	public double valorFinal(double valor) {
		return valor * quantidadeCompra + IMPOSTO / 2;
	}

}
